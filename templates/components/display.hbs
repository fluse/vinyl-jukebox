<section id="display" class="wdt-30">
    <span v-on:click="play">play</span>
    <span v-on:click="pause">pause</span>
    <span v-on:click="next">next</span>

    <div class="grd-1">
        <ul style="float: left; width: 100%;">
            <li v-for="track in queue" v-on:click="playTrack(track.uri)" style="float: left;">
                <img :src="track.album.images[0].url" width="200"/>
                \{{track.name}} \{{track.album.name}}
            </li>
        </ul>
    </div>

</section>
